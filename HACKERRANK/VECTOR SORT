https://www.hackerrank.com/challenges/vector-sort/problem?isFullScreen=true


#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int n;
    cin>>n;
    // declaring a vector named v.
    vector<int>v;
    // element
    int el;
    // Now inserting the elements in the vector
    for(int i=0; i<n; i++){
        cin>>el;
        // inserting the elements in vector
        v.push_back(el);
    }
    // using the sort function
    sort(v.begin(),v.end());
    // printing the sorted vector
    for(int i=0; i<n; i++){
        cout<<v[i]<<" ";
    }
    return 0;
}
