#include<bits/stdc++.h>
using namespace std;
int main(){
        int a,b;
        float c;
        cout<<"Enter the value."<<endl;
        cin>>a>>b;
        cin>>c;
                if(a>b){
                        if(a>c){
                                cout<<" a is greater ";
                        }
                        else 
                                cout<<" c is greater ";
                                
                }
                else{
                        if(b>a){
                                if (b>c)
                                cout<<" b is greater";
                        }
                        else
                        cout<<" c is greater ";
                }

        return 0;
}
       
