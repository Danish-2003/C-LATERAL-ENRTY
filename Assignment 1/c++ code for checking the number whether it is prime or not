#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cout<<"Enter the value of n :\n";
    cin>>n;
    int count=0;
    for (int i=1; i<=n; i++ )
    {
      if(n%i==0){
        count++;
      }

    }
    if(count==2){
        cout<<" The number is prime ";

    }
    else{
        cout<<" The number is NOT PRIME ";
    }
    
    return 0;

}
