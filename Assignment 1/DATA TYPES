
#include <bits/stdc++.h>
using namespace std;

int main(){
    float a=59.98;
    int b=5;
    float sum=a+b;
    cout<<"The value of a+b is "<<sum;
    return 0;
}

#include <bits/stdc++.h>
using namespace std;

int main(){
    int a=59;
    float b=5.8939;
    float sum = a-b;
    cout<<"The value of a-b is "<<sum;
    return 0;
}


#include <bits/stdc++.h>
using namespace std;

int main(){
    int a=59;
    float b=5.8939;
    float sum = a*b;
    cout<<"The value of a* is "<<sum;
    return 0;
}
