#include <bits/stdc++.h>
using namespace std;

int main(){
    int x,y,z;
    cin>>x>>y>>z;
    cout<<x<<endl;
    cout<<y<<endl;
    cout<<z;
    cout<<endl;
    if (x>y&&x>z)
    {
      cout<<"x is greater " ;
    }
    else if (y>x&&y>z)
    {
       cout<<"y is greater";

    }
    else if(z>x&&z>y){
        cout<<"z is greater";
    }
    


    return 0;
}
